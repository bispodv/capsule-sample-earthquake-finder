capsule {
	id (example.earthquakeFinder)
	version (0.7.5)
	format (3)
	targets {
		target (bixby-mobile-en-US)
	}
	capsule-imports {
		import (viv.time) {
			as (time) version (3.1.188)
		}
		import (viv.geo) {
			as (geo) version (9.26.48)
		}
	}
	permissions {
		device-location-access
	}
	store-sections {
		section (EducationAndReference)
	}
	runtime-version (7) {
		overrides {
			no-fallback-dialog-in-views (false)
		}
	}
	store-countries {
		only {
			allow (US)
		}
	}
}